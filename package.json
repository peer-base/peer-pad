{
  "name": "peerpad",
  "version": "0.3.1",
  "homepage": "./",
  "private": true,
  "leadMaintainer": "Jim Pick <jim@jimpick.com>",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "GIT_COMMIT=$(git rev-parse HEAD) node scripts/build.js",
    "lint": "standard 'src/**/*.js' 'test/**/*.js'",
    "test": "jest --no-cache --env=jsdom",
    "test:e2e": "jest --testMatch **/test/e2e/**/*.js",
    "test:e2e:ci": "NO_SANDBOX=true URL=http://localhost:1337 run-p --race test:serve test:e2e",
    "test:e2e:load": "node test/e2e-load",
    "test:serve": "http-server -s -p 1337 build",
    "serve:build": "http-server -p 1337 build",
    "deploy:ipfs": "ipfs add -r build/",
    "start:rendezvous": "rendezvous",
    "start:test:pinner": "pinner peer-pad/2 /dns4/127.0.0.1/tcp/9090/ws/p2p-websocket-star"
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^6.1.0",
    "bfj": "6.1.1",
    "bs58": "^4.0.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.30.0",
    "color-hash": "^1.0.3",
    "css-loader": "1.0.0",
    "dotenv": "6.1.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "fast-diff": "^1.1.2",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "katex": "^0.10.0-beta",
    "libp2p-crypto": "^0.14.1",
    "lodash.debounce": "^4.0.8",
    "milliseconds": "^1.0.3",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "peer-star-app": "^0.10.0",
    "pify": "^4.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.6.0",
    "quill": "^1.3.2",
    "quill-delta-to-html": "^0.10.6",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.1.1",
    "remark": "^10.0.1",
    "remark-html": "^9.0.0",
    "remark-html-katex": "^1.1.1",
    "remark-math": "^1.0.4",
    "style-loader": "0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tachyons": "^4.8.1",
    "tachyons-flexbox": "^2.0.5",
    "tachyons-forms": "^3.1.3",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "delay": "^4.1.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "http-server": "^0.11.1",
    "libp2p-websocket-star-rendezvous": "^0.2.4",
    "puppeteer": "^1.9.0",
    "puppeteer-cluster": "^0.12.1",
    "react-test-renderer": "^16.6.3",
    "rollbar": "^2.5.0",
    "rollbar-sourcemap-webpack-plugin": "^2.4.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|styl)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|styl|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|styl)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|styl)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
